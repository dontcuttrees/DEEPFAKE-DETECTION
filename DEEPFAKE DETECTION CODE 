        "\n",
        "uploaded = files.upload()\n",
        "image_path = list(uploaded.keys())[0]\n",
        "\n",
        "label, confidence = predict_image(image_path)\n",
        "\n",
        "print(\"Prediction:\", label)\n",
        "print(\"Confidence:\", confidence, \"%\")\n"
