    {
      "cell_type": "code",
      "source": [
        "import cv2\n",
        "import os\n",
        "\n",
        "def extract_frames_from_folder(real_input, output_folder, num_frames=10):\n",
        "    # Create output folder if it doesn't exist\n",
        "        os.makedirs(output_folder, exist_ok=True)\n",
        "\n",
        "            # List all video files\n",
        "        videos = os.listdir(real_input)\n",
        "\n",
        "        for video in videos:\n",
        "            video_path = os.path.join(real_input, video)\n",
        "            cap = cv2.VideoCapture(video_path)\n",
        "\n",
        "            total = int(cap.get(cv2.CAP_PROP_FRAME_COUNT))\n",
        "            interval = max(total // num_frames, 1)\n",
        "\n",
        "            video_name = video.split(\".\")[0]\n",
        "            video_output_folder = os.path.join(output_folder, video_name)\n",
        "            os.makedirs(video_output_folder, exist_ok=True)\n",
        "\n",
        "            count = 0\n",
        "            saved = 0\n",
        "            while cap.isOpened() and saved < num_frames:\n",
        "                  ret, frame = cap.read()\n",
        "                  if not ret:\n",
        "                        break\n",
        "\n",
        "                  if count % interval == 0:\n",
        "                        frame = cv2.resize(frame, (224, 224))\n",
        "                        cv2.imwrite(\n",
        "                              os.path.join(video_output_folder, f\"frame_{saved}.jpg\"),\n",
        "                              frame\n",
        "                        )\n",
        "                        saved += 1\n",
        "\n",
        "                  count += 1\n",
        "\n",
        "        cap.release()\n",
        "\n",
        "print(\"Frame extraction complete.\")\n"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "J8v_oMQFoSxp",
        "outputId": "bac03aef-f7ce-4c35-9d76-0bfa30df98f5"
      },
      "execution_count": 125,
      "outputs": [
        {
          "output_type": "stream",
