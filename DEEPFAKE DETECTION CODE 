    {
      "cell_type": "code",
      "source": [
        "import os\n",
        "from PIL import Image\n",
        "import torch\n",
        "from torch.utils.data import Dataset\n",
        "from torchvision import transforms\n",
        "\n",
        "class FrameDataset(Dataset):\n",
        "      def __init__(self, real_dir, fake_dir):\n",
        "        self.samples = []\n",
        "\n",
        "        self.transform = transforms.Compose([\n",
        "        transforms.ToTensor(),\n",
        "        transforms.Normalize([0.485, 0.456, 0.406],\n",
        "                           [0.229, 0.224, 0.225])\n",
        "        ])\n",
        "        # Real frames (label 0)\n",
        "        for folder in os.listdir(real_dir):\n",
        "          folder_path = os.path.join(real_dir, folder)\n",
        "          for img in os.listdir(folder_path):\n",
        "            self.samples.append((os.path.join(folder_path, img), 0))\n",
        "       # Fake frames (label 1)\n",
        "        for folder in os.listdir(fake_dir):\n",
        "          folder_path = os.path.join(fake_dir, folder)\n",
        "          for img in os.listdir(folder_path):\n",
        "            self.samples.append((os.path.join(folder_path, img), 1))\n",
        "      def __len__(self):\n",
        "        return len(self.samples)\n",
        "      def __getitem__(self, idx):\n",
        "            img_path, label = self.samples[idx]\n",
        "            image = Image.open(img_path).convert(\"RGB\")\n",
        "            image = self.transform(image)\n",
        "            return image, torch.tensor(label)\n"
      ],
      "metadata": {
        "id": "jsc5myQNxXI_"
      },
      "execution_count": 130,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "real_frames = \"/content/drive/MyDrive/deepfake_project/real_frames\"\n",
        "fake_frames = \"/content/drive/MyDrive/deepfake_project/fake_frames\"\n",
        "\n",
        "dataset = FrameDataset(real_frames, fake_frames)\n",
        "\n",
        "print(\"Total frames:\", len(dataset))\n"
      ],
      "metadata": {
